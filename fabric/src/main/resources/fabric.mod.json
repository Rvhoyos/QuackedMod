{
  "schemaVersion": 1,
  "id": "quack",
  "version": "${version}",
  "name": "QuackedDucks",
  "description": "New duck entity that resemble a rubber duck with custom items such as meat, cooked meat, foie gras with buffs and custom Duck AI behavior on land and water (flying coming soon!)",
  "authors": [
    "rvhoyos"
  ],
  "contact": {
    "homepage": "https://github.com/rvhoyos", 
    "issues": "https://github.com/rvhoyos/QuackMod/issues",
    "sources": "https://github.com/rvhoyos/QuackMod"
  },
  "license": "All Rights Reserved",
  "icon": "assets/quack/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "mc.quackedducks.fabric.QuackModFabric"
    ],
    "client": [
      "mc.quackedducks.fabric.client.QuackModFabricClient"
    ]
  },
  "mixins": [
    "quack.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.2",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "architectury": ">=17.0.8",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
